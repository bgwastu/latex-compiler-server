    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/multer": "^1.4.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.0",
