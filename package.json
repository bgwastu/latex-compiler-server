{
  "name": "latex-compiler-server",
  "version": "1.0.0",
  "description": "A LaTeX compilation server using Express.js",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec \"node --loader ts-node/esm\" src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:examples": "node --loader ts-node/esm src/test-examples.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "fs-extra": "^11.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/multer": "^1.4.0",
    "@types/fs-extra": "^11.0.0",
    "@types/uuid": "^9.0.0",
    "@types/jest": "^29.0.0",
    "@types/supertest": "^2.0.0",
    "ts-node": "^10.9.0"
  },
  "keywords": ["latex", "express", "pdf", "compiler"],
  "author": "",
  "license": "MIT"
}